class Solution(object):
    def buildArray(self, nums):
        """
        :type nums: List[int]
        :rtype: List[int]
        """

        for i in range(len(nums)):
            return [nums[nums[i]] for i in range(len(nums))]
        

solution = Solution()
num = [0,2,1,5,3,4]
ans = solution.buildArray(num)
print(ans)




   
